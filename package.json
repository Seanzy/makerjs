{
  "name": "@makerdao/makerdao-exchange-integration",
  "version": "0.0.19",
  "contributors": [
    "Wouter Kampmann <wouter@makerdao.com>",
    "Sean Brennan <sean@makerdao.com>",
    "Tyler Sorensen <tyler@makerdao.com>",
    "Ethan Bennett <ethan@makerdao.com>"
  ],
  "description": "MakerDAO exchange integration library.",
  "license": "MIT",
  "keywords": [
    "makerdao",
    "mkr",
    "dai",
    "cdp",
    "eth",
    "margin"
  ],
  "bugs": {
    "url": "https://github.com/wkampmann/makerdao-integration-poc/issues",
    "email": "wouter@makerdao.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wkampmann/makerdao-integration-poc.git"
  },
  "module": "src/index.js",
  "scripts": {
    "build": "webpack --env=prod --progress --profile --colors",
    "coverage": "jest --coverage --config=conf/jest.config.js",
    "lint": "eslint ./src/**.js ./src/**/**.js ./test/**.js ./test/**/**.js",
    "precommit": "lint-staged",
    "prepush": "npm run test",
    "prettier": "find ./src/**/*.js | xargs prettier --write --single-quote es5",
    "start": "webpack-dev-server --config conf/webpack.dev.js",
    "test": "./with-deployed-system jest --config=conf/jest.config.js --runInBand",
    "test:watch": "./with-deployed-system jest --config=conf/jest.config.js --watch --runInBand",
    "debugOasis": "./with-deployed-system",
    "testnet": "./node_modules/.bin/ganache-cli -i 999 -p 2000 -a 2 -m 'hill law jazz limb penalty escape public dish stand bracket blue jar'"
  },
  "lint-staged": {
    "./src/**/*.js": [
      "prettier --single-quote es5 --write",
      "git add",
      "eslint ./src/**.js ./src/**/**.js ./test/**.js ./test/**/**.js"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.5",
    "eslint": "^4.5.0",
    "eslint-loader": "^1.9.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "ganache-cli": "^6.1.0",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "lint-staged": "^7.0.0",
    "node-sass": "^4.5.3",
    "prettier": "^1.11.1",
    "sass-loader": "^6.0.6",
    "solc": "^0.4.19",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "bignumber.js": "^5.0.0",
    "bunyan": "^1.8.12",
    "ethers": "^2.2.6",
    "ethers-web3-bridge": "0.0.1",
    "web3": "^0.20.2"
  }
}
