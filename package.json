{
  "name": "@makerdao/makerdao-exchange-integration",
  "version": "0.0.39",
  "contributors": [
    "Wouter Kampmann <wouter@makerdao.com>",
    "Sean Brennan <sean@makerdao.com>",
    "Tyler Sorensen <tyler@makerdao.com>",
    "Ethan Bennett <ethan@makerdao.com>"
  ],
  "description": "MakerDAO exchange integration library.",
  "license": "MIT",
  "keywords": [
    "makerdao",
    "mkr",
    "dai",
    "cdp",
    "eth",
    "margin"
  ],
  "bugs": {
    "url": "https://github.com/wkampmann/makerdao-integration-poc/issues",
    "email": "wouter@makerdao.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wkampmann/makerdao-integration-poc.git"
  },
  "module": "src/index.js",
  "main": "src/index.js",
  "scripts": {
    "build:frontend": "webpack --env=prod --progress --profile --colors",
    "build:backend": "rm -rf dist && npm shrinkwrap && babel contracts --out-dir ./dist/contracts && babel src --out-dir ./dist/src && copyfiles README.md LICENSE package.json npm-shrinkwrap.json contracts/abi/**/* contracts/abi/dai/**/* src/utils/configs/* dist",
    "coverage": "jest --coverage --config=conf/jest.config.js --runInBand",
    "lint": "eslint './web/**.js' './src/**.js' './src/**/**.js' './test/**.js' './test/**/**.js'",
    "precommit": "lint-staged",
    "prepush": "npm run test",
    "prettier": "find ./src/**/*.js | xargs prettier --write --single-quote es5",
    "start": "webpack-dev-server --config conf/webpack.dev.js",
    "test": "./with-deployed-system --ci jest --config=conf/jest.config.js --runInBand",
    "test:watch": "./with-deployed-system jest --config=conf/jest.config.js --watch --runInBand",
    "test:logs": "tail -f ganache.out | grep -v 'eth_blockNumber'",
    "test:net": "./with-deployed-system"
  },
  "lint-staged": {
    "./{src,web,test}/**/*.js": [
      "prettier --single-quote es5 --write",
      "git add",
      "eslint './web/**.js' './src/**.js' './src/**/**.js' './test/**.js' './test/**/**.js'"
    ]
  },
  "devDependencies": {
    "0x.js": "^0.37.2",
    "@0xproject/connect": "^0.6.12",
    "@0xproject/subproviders": "^0.10.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "copyfiles": "^2.0.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-loader": "^1.9.0",
    "ethereumjs-util": "^5.2.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "ganache-cli": "^6.0.3",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "lint-staged": "^7.1.0",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.12.1",
    "sass-loader": "^6.0.7",
    "solc": "^0.4.23",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "vue": "^2.5.16",
    "vue-loader": "^15.0.9",
    "vue-template-compiler": "^2.5.16",
    "web3-provider-engine": "^14.0.5",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.2",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "bignumber.js": "^5.0.0",
    "bunyan": "^1.8.12",
    "ethers": "^3.0.15",
    "ethers-web3-bridge": "0.0.1",
    "web3": "^0.20.6"
  }
}
